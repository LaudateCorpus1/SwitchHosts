    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.6",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@types/assert": "^1.5.5",
    "@types/codemirror": "^5.60.2",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.172",
    "@types/md5": "^2.3.1",
    "@types/mkdirp": "^1.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^15.6.1",
